</elementSpec><elementSpec module="core" ident="hi">
  <equiv/>
  <gloss>highlighted</gloss>
  <gloss version="2007-12-20" xml:lang="kr">강조체</gloss>
  <gloss version="2007-05-02" xml:lang="zh-tw">顯目標示</gloss>
