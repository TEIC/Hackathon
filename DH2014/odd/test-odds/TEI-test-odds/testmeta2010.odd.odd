            </oneOrMore>
            <optional>
              <choice>
                
                <oneOrMore>
                  <group>
                    <group>
                      <ref name="model.divLike"/>
                    </group>
                    <zeroOrMore>
                      <choice>
                        <ref name="model.global"/>
                        <ref name="model.divGenLike"/>
                      </choice>
                    </zeroOrMore>
                  </group>
                </oneOrMore>
                
                <oneOrMore>
                  <group>
                    <group>
                      <ref name="model.div1Like"/>
                    </group>
                    <zeroOrMore>
                      <choice>
                        <ref name="model.global"/>
                        <ref name="model.divGenLike"/>
                      </choice>
                    </zeroOrMore>
                  </group>
                </oneOrMore>
              </choice>
            </optional>
          </group>
        </choice>
        
      </group>
      
      <zeroOrMore>
        <group>
          <group>
            <ref name="model.divBottom"/>
          </group>
          <zeroOrMore>
            <ref name="model.global"/>
          </zeroOrMore>
        </group>
      </zeroOrMore>
    </group>
  </content>
  <exemplum xml:lang="mul">
