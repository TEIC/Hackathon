   </desc><classes><memberOf key="att.global"/></classes><content><group xmlns="http://relaxng.org/ns/structure/1.0"><ref name="teiHeader"/><choice><group><oneOrMore><ref name="model.resourceLike"/></oneOrMore><optional><ref name="text"/></optional></group><ref name="text"/></choice></group></content><constraintSpec ident="c1" scheme="isoschematron">
