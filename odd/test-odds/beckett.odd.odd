                        <valItem ident="ink"/>
                        <valItem ident="pencil"/>
                     </valList>
                  </attDef><attDef ident="style" mode="delete"/><attDef ident="rendition" mode="delete"/><attDef ident="sameAs" mode="delete"/><attDef ident="facs" mode="delete"/><attDef ident="place" mode="change">
                     <valList mode="add" type="closed">
                        <valItem ident="crossed"/>
                        <valItem ident="facingleaf"/>
                        <valItem ident="infralinear"/>
                        <valItem ident="inline"/>
